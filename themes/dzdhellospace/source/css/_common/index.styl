.display-flex {
  display: flex;
  align-items: center;
  &.item-start {
    justify-content: flex-start;
  }
  &.item-end {
    justify-content: flex-end;
  }
  &.item-center {
    justify-content: center;
  }
  &.item-space-between {
    justify-content: space-between;
  }
  .grow-0 {
    flex-grow: 0;
  }
  .grow-1 {
    flex-grow: 1;
  }
  .shrink-0 {
    flex-shrink: 0;
  }
  .shrink-1 {
    flex-shrink: 1;
  }
  &.flex-end {
    justify-content: flex-end;
  }
  .flex-end {
    align-self: flex-end;
  }
  .flex-start {
    align-self: flex-start;
  }
}

